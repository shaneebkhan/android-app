<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

   <div
      class="o_mail_component_DialogManager"
      owl="1"
      t-name="mail.component.DialogManager">
      <Dialog
         class="o_mail_component_DialogManager__dialog"
         Component="dialog.Component"
         id="dialog.id"
         info="dialog.info"
         t-as="dialog"
         t-foreach="props.dialogs"
         t-key="dialog.id"
         t-on-close.stop="_onCloseDialog"/>
   </div>

</templates>
