<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <div t-name="mail.wip.widget.ThreadPreviewList" owl="1"
         class="o_mail_wip_thread_preview_list">
        <t t-foreach="props.threadLIDs"
           t-as="threadLID"
           t-widget="ThreadPreview"
           t-key="threadLID"
           t-props="{ threadLID }"
           t-on-click="_onClickPreview"/>
    </div>

</templates>
