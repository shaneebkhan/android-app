<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <div t-name="mail.wip.widget.DiscussMobileNavbar" owl="1"
         class="o_mobile_navbar">
        <div class="o_tab"
             t-att-class="active === 'mailbox' ? 'o_active' : ''"
             t-on-click="_onClick('mailbox')">
             <span class="fa fa-inbox o_icon"/>
             <span class="o_label">Mailboxes</span>
        </div>
        <div class="o_tab"
             t-att-class="active === 'chat' ? 'o_active' : ''"
             t-on-click="_onClick('chat')">
             <span class="fa fa-user o_icon"/>
             <span class="o_label">Chat</span>
        </div>
        <div class="o_tab"
             t-att-class="active === 'channel' ? 'o_active' : ''"
             t-on-click="_onClick('channel')">
             <span class="fa fa-users o_icon"/>
             <span class="o_label">Channels</span>
        </div>
    </div>

</templates>
